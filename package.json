  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.15",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.0",
    "jest-circus": "^26.6.3",
    "semantic-release": "^17.2.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@types/glob": "^7.1.3",
    "@vercel/ncc": "^0.25.1",
    "discord.js": "^12.4.1",
    "glob": "^7.1.6"
  },
  "jest": {
    "testRunner": "jest-circus/runner"
  },
  "commitlint": {
    "extends": [
