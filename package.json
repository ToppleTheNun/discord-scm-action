  "main": "dist/index.js",
  "module": "dist/discord-scm-action.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdx build",
    "commit": "git-cz",
